{
	"presets": [
		["@babel/preset-env", {"useBuiltIns": "usage", "targets": {
			"node": "current"
		}}
		],"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal"}],
		"@babel/plugin-transform-runtime"
	]
}
